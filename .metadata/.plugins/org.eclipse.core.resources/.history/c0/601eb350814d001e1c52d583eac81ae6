package com.example.springweb;

public class Ex_MultiThreads implements Runnable{
	
	public void run() {
		
		System.out.println("Thread is running");
	}
	
	public static void main(String args[]) {
		
		Runnable r1=new Ex_MultiThreads(); //up casting
		Thread t1=new Thread(r1,"Thread 1"); //Thread constructor - Passing runnable object and String value
		System.out.println(t1.getName() + " is " + t1.getState());
		t1.start();
		System.out.println(t1.getName() + " is " +t1.getState());
		
		
		
	}

}
